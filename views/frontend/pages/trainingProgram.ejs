            <!-- <div><%= datas[0].author %></div> -->

            <div class="trainingProgram-container">
                <ul class="row">
                <% for(var i = 0; i <datas.length; i++){ %>
                        <li class="col-lg-3 col-md-4 col-sm-6 col-xs-6" onclick="javascript:location.href='courseInfo/'+'<%=datas[i].id %>'" style="cursor: pointer;">
